{
    "name": "SimNML",
    "displayName": "SimNML",
    "description": "Syntax HighLighting for SimNML",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "simnml",
                "aliases": [
                    "simnml",
                    "simnml"
                ],
                "extensions": [
                    ".nmp",
                    ".nml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "simnml",
                "scopeName": "source.nmp",
                "path": "./syntaxes/simnml.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
